---
import IndexLayout from "@/layouts/IndexLayout.astro";
import { themeConfig } from "@/config";
import PublicationEntry from "@/components/widgets/PublicationEntry.astro";
import codesignGif from "@/media/codesign.gif";
import framediffGif from "@/media/framediff.gif";
import frameflowGif from "@/media/frameflow.gif";
import octaneGif from "@/media/octane_s.gif";
import ggsPng from "@/media/ggs.png";
import afmPng from "@/media/afm.png";
import rfdiffusionGif from "@/media/rfdiff.gif";
import rfdiffusion2Gif from "@/media/rfdiff2.gif";
import bioemuGif from "@/media/bioemu.gif";
import metalPng from "@/media/metallo.jpg";
---

<IndexLayout
    title={`Publications - ${themeConfig.site.title}`}
    description={themeConfig.site.description}
>
    <p>
        These are selected publications. For a full list of my publications, see my
        <a
            href="https://scholar.google.com/citations?user=8wDe9NAAAAAJ"
            target="_blank"
            rel="noopener noreferrer"
        >
            Google Scholar profile
        </a>
        .
        <br/>
        * denotes equal contribution.
    </p>

    <section class="pub-section">
        <h1>Machine learning</h1>
        <PublicationEntry
            media={{ src: codesignGif.src, alt: "Protein co-design visualization" }}
            title="Generative Flows on Discrete State-Spaces: Enabling Multimodal Flows with Applications to Protein Co-Design"
            authors="Andrew Campbell*, Jason Yim*, Regina Barzilay, Tom Rainforth, Tommi Jaakkola"
            venue="International Conference of Machine Learning 2024"
            descriptionEntry="codesign"
            description=""
            links={[
                {
                    href: "https://arxiv.org/abs/2402.04997",
                    label: "Paper",
                },
                {
                    href: "https://www.youtube.com/watch?v=-L9ekyTI21k",
                    label: "Talk",
                },
                {
                    href: "https://github.com/jasonkyuyim/multiflow",
                    label: "Multimodal Flow Code",
                },
                {
                    href: "https://github.com/andrew-cr/discrete_flow_models",
                    label: "Discrete Flow Code",
                },
            ]}
        />
        <PublicationEntry
            media={{ src: framediffGif.src, alt: "Frame-diff visualization" }}
            title="SE (3) diffusion model with application to protein backbone generation"
            authors="Jason Yim*, Brian L Trippe*, Valentin De Bortoli*, Emile Mathieu*, Arnaud Doucet, Regina Barzilay, and Tommi Jaakkola"
            venue="International Conference of Machine Learning 2023"
            descriptionEntry="framediff"
            description=""
            links={[
                {
                    href: "https://arxiv.org/abs/2302.02277",
                    label: "Paper",
                },
                {
                    href: "https://news.mit.edu/2023/generative-ai-imagines-new-protein-structures-0712",
                    label: "MIT News",
                },
                {
                    href: "https://www.youtube.com/watch?v=IXL4WCLFeZM",
                    label: "Talk",
                },
                {
                    href: "https://github.com/jasonkyuyim/se3_diffusion",
                    label: "Code",
                },
            ]}
        />
        <PublicationEntry
            media={{ src: frameflowGif.src, alt: "Frame-flow visualization" }}
            title="Improved motif-scaffolding with SE (3) flow matching"
            authors="Jason Yim, Andrew Campbell, Emile Mathieu, Andrew YK Foong, Michael Gastegger, José Jiménez-Luna, Sarah Lewis, Victor Garcia Satorras, Bastiaan S Veeling, Frank Noé, and  others"
            venue="Transactions on Machine Learning Research 2024"
            descriptionEntry="frameflow"
            description=""
            links={[
                {
                    href: "https://arxiv.org/abs/2310.05297",
                    label: "Paper",
                },
                {
                    href: "https://github.com/microsoft/protein-frame-flow",
                    label: "Code",
                },
            ]}
        />
        <PublicationEntry
            media={{ src: ggsPng.src, alt: "GGS visualization" }}
            title="Improving protein optimization with smoothed fitness landscapes"
            authors="Andrew Kirjner*, Jason Yim*, Raman Samusevich, Shahar Bracha, Tommi Jaakkola, Regina Barzilay, and Ila Fiete"
            venue="International Conference on Learning Representations 2024"
            descriptionEntry="ggs"
            description=""
            links={[
                {
                    href: "https://arxiv.org/abs/2307.00494",
                    label: "Paper",
                },
                {
                    href: "https://news.mit.edu/2024/new-computational-technique-could-help-engineer-useful-proteins-0403",
                    label: "MIT News",
                },
                {
                    href: "https://github.com/kirjner/GGS",
                    label: "Code",
                },
            ]}
        />
    </section>

    <section class="pub-section">
